  "bugs": {
    "url": "https://github.com/heyjul3s/artifak/issues"
  },
  "dependencies": {
    "@artifak/component-generator": "^2.0.4",
    "@artifak/container": "^1.0.1",
    "@artifak/flex": "^1.1.7",
    "@artifak/fluidsizing": "^1.0.2",
    "@artifak/grid": "^1.1.5",
    "@artifak/hextorgb": "^1.0.1",
    "@artifak/hextorgba": "^1.0.1",
    "@artifak/imagery": "^1.0.7",
    "@artifak/media": "^1.0.2",
    "@artifak/pxtoem": "^1.0.1",
    "@artifak/pxtorem": "^1.0.1",
    "@artifak/text-input": "^1.0.3",
    "@artifak/typography": "^2.0.2",
    "@artifak/useclickaway": "^1.0.1",
    "@artifak/usedebouncedfn": "^1.0.1",
    "@artifak/usematchmedia": "^1.0.4",
    "@artifak/usewindowsize": "^1.0.4"
  },
  "devDependencies": {
    "@artifak/bundler": "^1.1.5"
  }
}
