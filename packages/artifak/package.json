{
  "name": "artifak",
  "version": "2.1.1",
  "description": "A set of React UI primitives to help you build your component UI system",
  "author": "Julian Low",
  "homepage": "https://github.com/heyjul3s/artifak#readme",
  "license": "MIT",
  "keywords": [
    "react",
    "react-component",
    "component",
    "ui",
    "flex",
    "grid",
    "block",
    "layout"
  ],
  "sideEffects": false,
  "main": "dist/artifak.cjs.js",
  "module": "dist/artifak.esm.js",
  "src": "src/index.tsx",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heyjul3s/artifak.git"
  },
  "scripts": {
    "build": "rimraf dist && builder",
    "depcheck": "npx depcheck",
    "test": "echo \"Error: run tests from root\" && exit 1"
